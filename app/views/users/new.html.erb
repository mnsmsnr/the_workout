<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>
<%= simple_form_for @user do |f| %>
  <%= render 'shared/error_messages' %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation ,label: "confirmation" %>
  <%= f.button :submit ,"Create my account"%>
<% end %>